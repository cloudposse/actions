    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "test": "jest unit",
    "test:int": "jest int"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peter-evans/slash-command-dispatch.git"
  },
